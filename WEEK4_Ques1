#include<iostream>
using namespace std;
void heapSort(int a[],int n);
void heapify(int a[],int n,int i);
int main()
{
    int t,n,*a=NULL;
    cin>>t;
    while(t--)
    {
        cin>>n;
        a=(int*)malloc(sizeof(int)*n);
        for(int i=0;i<n;i++) cin>>a[i];
        heapSort(a,n);
        for(int i=0;i<n;i++) cout<<a[i]<<' ';
    }
    return 0;
}
void heapSort(int a[],int n)
{
    for(int i=n/2-1;i>=0;i--) heapify(a,n,i);
    for(int i=n-1;i>0;i--)
    {
        int t=a[i];
        a[i]=a[0];
        a[0]=t;
        heapify(a,i,0);
    }
}
void heapify(int a[],int n,int i)
{
    int c1=2*i+1,c2=2*i+2,m=i,t;
    if(c1<n&&a[c1]>a[m])
        m=c1;
    if(c2<n&&a[c2]>a[m])
        m=c2;
    if(m!=i)
    {
        t=a[m];
        a[m]=a[i];
        a[i]=t;
        heapify(a,n,m);
    }
}
