#include<iostream>
#include<vector>
using namespace std;
int f=0;
void dfs(int s,int d,vector<bool> &vis,vector<vector<int> > g);
int main()
{
    int t,s,d;
    cin>>t;
    vector<vector<int> > vec(t,vector<int>(t,0));
    for(int i=0;i<t;i++){
        for(int j=0;j<t;j++){
            cin>>s;
            vec[i][j]=s;
        }
    }
    cin>>s>>d;
    vector<bool> vis(t,false);
    dfs(s-1,d-1,vis,vec);
    if(f==1) cout<<"There is a path from "<<s<<" to "<<d<<".\n";
    else cout<<"There is no path from "<<s<<" to "<<d<<".\n";
    return 0;
}
void dfs(int s,int d,vector<bool> &vis,vector<vector<int> > g){
    if(s==d) f=1;
    vis[s]=true;
    for(int i=0;i<g[s].size();i++)
        if(!vis[i]&&g[s][i]!=0)
            dfs(i,d,vis,g);
}
