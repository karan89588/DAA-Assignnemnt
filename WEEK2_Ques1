#include<iostream>
#include<stdlib.h>
using namespace std;
int times(int a[],int l,int r,int k);
int main(){
    int t,n,key,index,i,j,c=1;
    int *a=NULL;
    cin>>t;
    while(t--){
        cin>>n;
        a=(int *)malloc(sizeof(int)*n);
        for(int k=0;k<n;k++) cin>>a[k];
        cin>>key;
        index=times(a,0,n-1,key);
        i=index;
        j=index;
        while(i!=-1&&i<n&&a[i]==a[i+1]){ i++; c++; }
         while(j!=-1&&j>0&&a[j]==a[j-1]){ j--; c++; }
        cout<<key<<"-"<<c<<endl;}
    return 0;}
int times(int a[],int l,int r,int k){
    if(l<=r){
        int mid=l+(r-l)/2;
        if(a[mid]>k) return times(a,l,mid-1,k);
        else if(a[mid]<k) return times(a,mid+1,r,k);
        else return mid;
    }
    return -1;}
