#include<iostream>
#include<stdlib.h>
using namespace std;
int finds(int a[],int n,int k);
int main()
{
    int t,n,*a=NULL,k,sol;
    cin>>t;
    while(t--)
    {
        cin>>n;
        a=(int*)malloc(sizeof(int)*n);
        for(int i=0;i<n;i++) cin>>a[i];
        cin>>k;
        sol=finds(a,n,k);
        if(sol!=-1) cout<<sol;
        else cout<<"No Sequence found.";
    }
    return 0;
}
int finds(int a[],int n,int k)
{
    int sol=0;
    for(int i=0;i<n;i++) for(int j=0;j<n;j++) if(abs(a[i]-a[j])==k) sol++;
    return sol/2;
}
