#include<iostream>
#include<stdlib.h>
using namespace std;
int selSort(int a[],int n);
int main(){
    int t,n,*a=NULL,k;
    cin>>t;
    while(t--){
        cin>>n;
        a=(int*)malloc(sizeof(int)*n);
        for(int i=0;i<n;i++) cin>>a[i];
        k=selSort(a,n);
        cout<<"Swap "<<k;
        cout<<"Comparison "<<k*n/2.0;}
    return 0;}
int selSort(int a[],int n){
    int pos,m,c=0;
    for(int i=0;i<n-1;i++){
        pos=i;
        m=a[i];
        for(int j=i+1;j<n;j++) if(m<a[j]) { m=a[j]; pos=j; }
        if(pos!=i){
            a[pos]=a[i];
            a[i]=m;
            c++;
        }
	}
    return c;}
