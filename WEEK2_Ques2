#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
vector< vector<int> > sequence(int a[],int n);
int main(){
    vector< vector<int> > v;
    int t,n,*a=NULL;
    cin>>t;
    while(t--){
        cin>>n;
        a=(int*)malloc(sizeof(int)*n);
        for(int i=0;i<n;i++) cin>>a[i];
        v=sequence(a,n);
        if(v.size()==0) cout<<"No Sequence is Possible.";
        else
            for(int i=0;i<v.size();i++){
                for(int j=0;j<v[i].size();j++) cout<<v[i][j]<<" ";
                cout<<endl;}}
    return 0;}
vector< vector<int> > sequence(int a[],int n){
    vector< vector<int> > v;
    if(n==1||n==0||n==2) return v;
    for(int k=n-1;k>=2;k--){
        int i=0,j=k-1;
        while(i<j){
            if(a[i]+a[j]>a[k]) j--;
            else if(a[i]+a[j]<a[k]) i++;
            else{
                vector<int> sol;
                sol.push_back(i);
                sol.push_back(j);
                sol.push_back(k);
                v.push_back(sol);
                i++;
                j--;}}}
    return v;}
