#include<iostream>
#include<stdlib.h>
using namespace std;
int insertionSort(int a[],int n);
int main(){
    int t,n,*a=NULL,k;
    cin>>t;
    while(t--){
        cin>>n;
        a=(int*)malloc(sizeof(int)*n);
        for(int i=0;i<n;i++) cin>>a[i];
        k=insertionSort(a,n);
        for(int i=0;i<n;i++) cout<<a[i]<<" ";
        cout<<"\nComparison "<<k;
        cout<<"Shift "<<n+k-1;
    }
    return 0;
}
int insertionSort(int a[],int n){
    int i,j,k,c=0;
    for(int i=1;i<n;i++){
        j=i-1;
        k=a[i];
        while(j>=0&&k<a[j]){
            a[j+1]=a[j];
            j--;
            c++;
        }
        a[j+1]=k;
    }
    return c;
}
