#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main(){
    int t,s;
    cin>>t;
    vector<pair<int,pair<int,int>>> data(t);
    for(int i=0;i<t;i++){
        cin>>s;
        data[i].second.first=s;
    }
    for(int i=0;i<t;i++){
        cin>>s;
        data[i].first=s;
        data[i].second.second=i+1;
    }
    sort(data.begin(),data.end());
    int take=1;
    int e=data[0].first;
    vector<int> v;
    v.push_back(1);
    for(int i=1;i<t;i++){
        if(data[i].second.first>=e){
            take++;
            e=data[i].first;
            v.push_back(data[i].second.second);
        }
    }
    cout<<"Total no of non Conflicting jobs : "<<take<<endl<<"And the jobs are : ";
    for(int i=0;i<v.size();i++)
        cout<<v[i]<<" ";
    return 0;
}
