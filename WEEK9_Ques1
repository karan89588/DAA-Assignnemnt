#include<iostream>
#define INF 99
#include<vector>
using namespace std;
void floydWarshall(vector<vector<int> > g,int t);
int main(){
    int t,s;
    cin>>t;
    vector<vector<int> > g(t,vector<int>(t,0));
    for(int i=0;i<t;i++){
        for(int j=0;j<t;j++){
            cin>>s;
            g[i][j]=s;
        }
    }
    floydWarshall(g,t);
    return 0;
}
void floydWarshall(vector<vector<int> > g,int t){
    for(int k=0;k<t;k++){
        for(int i=0;i<t;i++){
            for(int j=0;j<t;j++){
                if(g[i][k]!=INF&&g[k][j]!=INF&&g[i][j]>g[i][k]+g[k][j])
                    g[i][j]=g[i][k]+g[k][j];
            }
        }
    }
    cout<<"Shortest all apir path : "<<endl;
    for(int i=0;i<t;i++){
        for(int j=0;j<t;j++)
            cout<<g[i][j]<<" ";
        cout<<endl;
    }
}
