#include<iostream>
#include<vector>
using namespace std;
vector<vector<int> > ways;
void check(int s,vector<int> coin,int sum,int tsum,vector<int> ans);
int main(){
    int t,s,sum,tsum=0;
    cin>>t;
    vector<int> coin(t);
    for(int i=0;i<t;i++){
        cin>>s;
        coin[i]=s;
    }
    cin>>sum;
    vector<int> ans;
    check(0,coin,sum,tsum,ans);
    cout<<"Total no of Required combination : "<<ways.size();
    return 0;
}
void check(int s,vector<int> coin,int sum,int tsum,vector<int> ans){
    if(sum<tsum)
        return;
    if(sum==tsum){
        ways.push_back(ans);
        return ;
    }
    for(int i=s;i<coin.size();i++){
        ans.push_back(coin[i]);
        check(i,coin,sum,tsum+coin[i],ans);
        ans.pop_back();
    }
}
