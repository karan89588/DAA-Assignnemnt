#include<iostream>
#include<stdlib.h>
using namespace std;
int linearSearch(int a[],int n,int k);
int main()
{
    int t,n,*a=NULL,k,r;
    cin>>t;
    while(t--)
    {
        cin>>n;
        a=(int*)malloc(sizeof(int)*n);
        for(int i=0;i<n;i++) cin>>a[i]; 
	    cin>>k;
        r=linearSearch(a,n,k);
        if(r==-1) cout<<"Not Present "<<r;
        else cout<<"Present "<<r;
    }
    return 0;
}
int linearSearch(int a[],int n,int k)
{
   	for(int i=0;i<n;i++) if(a[i]==k) return i;
   	return -1; 
}
